<?xml version="1.0" encoding="UTF-8"?>
<rqt_mrta>
  <name>@ARCHITECTURE_NAME@</name>
  <robots>
    <type>@ROBOT_TYPE@</type>
    <idle_robots>
      <topic>
        <name>@IDLE_ROBOTS_TOPIC_NAME@</name>
        <type>@IDLE_ROBOTS_TOPIC_TYPE@</type>
        <field>@IDLE_ROBOTS_TOPIC_FIELD@</field>
        <timeout>@IDLE_ROBOTS_TOPIC_TIMEOUT@</timeout>
        <horizon>@IDLE_ROBOTS_TOPIC_HISTORY_LENGTH@</horizon>
      </topic>
    </idle_robots>
    <busy_robots>
      <topic>
        <name>@BUSY_ROBOTS_TOPIC_NAME@</name>
        <type>@BUSY_ROBOTS_TOPIC_TYPE@</type>
        <field>@BUSY_ROBOTS_TOPIC_FIELD@</field>
        <timeout>@BUSY_ROBOTS_TOPIC_TIMEOUT@</timeout>
        <horizon>@BUSY_ROBOTS_TOPIC_HISTORY_LENGTH@</horizon>
      </topic>
    </busy_robots>
  </robots>
  <tasks>
    <type>@TASK_TYPE@</type>
    <incoming_tasks>
      <topic>
        <name>@INCOMING_TASKS_TOPIC_NAME@</name>
        <type>@INCOMING_TASKS_TOPIC_TYPE@</type>
        <field>@INCOMING_TASKS_TOPIC_FIELD@</field>
      </topic>
    </incoming_tasks>
  </tasks>
  <allocations>
    <type>@ALLOCATION_TYPE@</type>
    <allocated_tasks>
      <topic>
        <name>@ALLOCATED_TASKS_TOPIC_NAME@</name>
        <type>@ALLOCATED_TASKS_TOPIC_TYPE@</type>
        <field>@ALLOCATED_TASKS_TOPIC_FIELD@</field>
        <timeout>@ALLOCATED_TASKS_TOPIC_TIMEOUT@</timeout>
        <horizon>@ALLOCATED_TASKS_TOPIC_HISTORY_LENGTH@</horizon>
      </topic>
    </allocated_tasks>
  </allocations>
</rqt_mrta>
